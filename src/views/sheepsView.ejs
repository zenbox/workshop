<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1.0" />
        <title>Sheeps</title>
    </head>
    <body>
        <h1><%= title %></h1>
        <ul>
            <% sheeps.forEach(function(entry) { %>
            <li><%= entry.sheep %></li>
            <% }); %>
        </ul>
    </body>
</html>
